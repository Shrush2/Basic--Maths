

class Main {
    public static void main(String[] args) {
        System.out.println(isPallindrome("madam"));
    }
    public static boolean isPallindrome(String s){
        if(s.length() <=1){
            return true;
        }
        
        for(int i =0;i<s.length() ;i++){
          if(s.charAt(i) != s.charAt(s.length()-i-1)){
            return false;
        }  
        }
        
        return true;
    }
}
