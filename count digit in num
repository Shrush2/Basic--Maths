import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        
        int cnt =0;
        while(num > 0){
            int ld = num % 10;
            cnt++;
            num = num / 10;
        }
        System.out.println("count digit in num "+cnt);
    }
}
