
import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
    
              System.out.println(printnum(num,1));
   
}
      
    public static int printnum(int num,int i){
         if(i > num){
             return 0;
         }
         System.out.println(i);
         return printnum(num,i+1);
    } 
}


ans-====== > 4
1 2 3 4 0
