import java.util.*;
class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int dup = num;
        int sum =0;
        while(num > 0){
            int ld = num % 10;
            num = num / 10;
            sum += Math.pow(ld,3);
        }
        if(dup == sum){
        System.out.println("the given num is Armstrong");
        } else{
        System.out.println("the number is not Armstrong");
     
        }
    }
}
153
the given num is Armstrong
